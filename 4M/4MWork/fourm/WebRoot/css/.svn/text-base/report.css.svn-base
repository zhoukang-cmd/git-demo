/* CSS Document */
body {
	background:#fff;
	color:#000;
	font-family:Microsoft YaHei, Arial, SimSun;
	font-size:12px;
	margin:0;
	padding:0
}
div, form, p, ol, dl, dt, dd, h1, h2, h3, h4, h5, h6, img {
	border:0;
	margin:0;
	padding:0
}
.sbox {
	width:2100px;
	margin:0 auto
}
table {
	border-left:1px solid #e5e5e5;
	border-top:1px solid #e5e5e5;
}
table td {
	border-right:1px solid #e5e5e5;
	border-bottom:1px solid #e5e5e5;
	height:25px;
}
.tab_auto {
	width:2100px;
	overflow:hidden;
}
.tab_auto td {
	text-align:center;
}
.report_bottons{ text-align:left; margin:15px 0}