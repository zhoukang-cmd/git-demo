/* 实时数据浮动窗口 */
.floatbox {position:absolute;left:10%;top:10%;width:80%;height:80%;z-index:1000;visibility:hidden;}
.floatbox .box {border:#036 solid 1px;width:95%; height:500px; background:#fff; color:#036; font-size:12px; line-height:150%;}
.floatbox .titlebar {background:#2996F1; height:30px;;font-size:12px; padding:3 5 0 10; color:#fff}
.floatbox .title {display:inline;float:left;font-size:14px;font-weight:bold;background-color: #2996F1;text-align:center;color:#fff;}
.floatbox .info {display:inline;float:left;font-size:14px;font-weight:bold;padding:5px 0 0 12px;color:#F00}
.floatbox .close {display:inline;float:right;font-size:14px;padding:5px 12px 0 0; cursor:pointer;}
.floatbox .group {width: 100%;height:100%;}
.floatbox .group dl {float:left;margin-right:20px;margin-left:20px;margin-top:10px;margin-bottom: 0px;}
.floatbox .group dd {width:151px;line-height:26px;border:1px solid #abccdf;padding-left:5px;margin-top:-1px;text-align:center;}
.title01 {padding:0px 0px 0px 21px;font-size:14px;font-weight:bold;line-height:30px;float:left;}
/*报表分析滚动选择*/
.rollBox {width:720px;overflow:hidden;margin:0 0 10px ;}
.rollBox .Cont{width:656px;overflow:hidden;margin:0 auto;padding-top:20px;float:left;}
.rollBox .ScrCont{width:10000000px;}
.rollBox .Cont {padding:6px 4px 4px;background:#F0F0F0;border:1px solid #ccc;display:block;margin:0 auto;height:30px;line-height:30px;}
.rollBox .Cont .pic{width:112px;float:left;text-align:center;font-size:13px;}

.rollBox .Cont a:link,.rollBox .Cont a:visited{color:#626466;text-decoration:none;}
.rollBox .Cont a:hover{color:#288DE7;text-decoration:underline;}
.rollBox #List{float:left;}

