<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMapConfig 
PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN" 
"http://ibatis.apache.org/dtd/sql-map-config-2.dtd">

<sqlMapConfig>
		
	<settings
         cacheModelsEnabled="false"
         enhancementEnabled="true"
         lazyLoadingEnabled="true"
         errorTracingEnabled="true"
         maxRequests="512"
         maxSessions="128"
         maxTransactions="32"
         useStatementNamespaces="true"
         defaultStatementTimeout="3600"
         statementCachingEnabled="true"
         classInfoCacheEnabled="true"
	/>
	<sqlMap resource="ibatis/oracle/user.xml" />
	<sqlMap resource="ibatis/oracle/suggest.xml" />
	<sqlMap resource="ibatis/oracle/fault.xml" />
	<sqlMap resource="ibatis/oracle/repair.xml" />
	<sqlMap resource="ibatis/oracle/field.xml" />
	<sqlMap resource="ibatis/oracle/equip.xml" />
</sqlMapConfig>
