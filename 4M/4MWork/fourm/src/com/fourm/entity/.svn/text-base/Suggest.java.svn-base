package com.fourm.entity;

/**
 * 发布结果
 * */
public class Suggest {

	private int suggId;
	private String suggCreateTime;
	private String suggTime;
	private String suggObject;
	private String suggContent;
	
	private int equipId;
	
	public Suggest(){}

	public int getSuggId() {
		return suggId;
	}

	public void setSuggId(int suggId) {
		this.suggId = suggId;
	}

	public String getSuggCreateTime() {
		return suggCreateTime;
	}

	public void setSuggCreateTime(String suggCreateTime) {
		this.suggCreateTime = suggCreateTime;
	}

	public String getSuggTime() {
		return suggTime;
	}

	public void setSuggTime(String suggTime) {
		this.suggTime = suggTime;
	}

	public String getSuggObject() {
		return suggObject;
	}

	public void setSuggObject(String suggObject) {
		this.suggObject = suggObject;
	}

	public String getSuggContent() {
		return suggContent;
	}

	public void setSuggContent(String suggContent) {
		this.suggContent = suggContent;
	}

	public int getEquipId() {
		return equipId;
	}

	public void setEquipId(int equipId) {
		this.equipId = equipId;
	}

	
}
